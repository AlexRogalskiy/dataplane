input AddUsersInput {
	username:  String!
	first_name: String!    
	last_name:  String!     
	email:     String!     
	password:  String!     
	timezone:  String!     
}

type User {
	user_id:   String! 
	username:  String!
	first_name: String!    
	last_name:  String!     
	email:     String!     
	timezone:  String!     
}

type Mutation {
  createUser(input: AddUsersInput): User
}