input AddPreferencesInput {
  preference: String!
  value: String!
}

type Preferences {
  preference: String!
  value: String!
}

extend type Query{
	getAllPreferences: [Preferences]
}

extend type Query{
	getOnePreference(preference: String!): Preferences
}

extend type Mutation {
  updatePreferences(input: AddPreferencesInput): String
}